TIPOS_NEGOCIO:
LOAD *	where Exists(PK_NEGOCIO);
LOAD TIPO_CARTERA, AutoNumber(TIPO_CARTERA, 'negocios') as PK_NEGOCIO, 
	"DESC_TIPO_CARTERA_CAST", "DESC_TIPO_CARTERA_CAT";
SQL SELECT "TIPO_CARTERA", "DESC_TIPO_CARTERA_CAST", 
	"DESC_TIPO_CARTERA_CAT"
FROM "TIPOS_NEGOCIO"; 

store 'TIPOS_NEGOCIO' into 'Extractor/TIPOS_NEGOCIO.qvd' (qvd);      

