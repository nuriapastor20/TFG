CAUSAS:
LOAD "PK_COD_CAUSA", "PK_COD_CAUSA" AS PK_COD_CAUSA_CAMPO, 
	"FK_NUM_ONG" , "COD_CONTRATO", "COD_AGRUPACION", "DES_CAUSA", "DES_AGRUPACION",
    IMPORTE1, IMPORTE2, IMPORTE3, IMPORTE4, "IMPORTE_MIN", 
    date(FECHA_INICIO_CAUSA,'DD-MM-YYYY') as FECHA_INICIO_CAUSA, 
    date(FECHA_FIN_CAUSA,'DD-MM-YYYY')  as FECHA_FIN_CAUSA;
    ;
SQL SELECT "PK_COD_CAUSA", "FK_NUM_ONG", "COD_CONTRATO", "COD_AGRUPACION", "DES_CAUSA", 
	"DES_AGRUPACION", IMPORTE1, IMPORTE2, IMPORTE3, IMPORTE4, "IMPORTE_MIN", 
    "FECHA_INICIO_CAUSA", "FECHA_FIN_CAUSA"
FROM "CAUSAS";

concatenate(CAUSAS) 

LOAD 
text(COD_BIZUM)& ' - ' & [NIF] as PK_COD_CAUSA,
'BIZ - '& text(COD_BIZUM) AS PK_COD_CAUSA_CAMPO,
ApplyMap('Map_Razon_Social',("NIF"),'Desconocido') as DES_CAUSA
resident DONACIONES_BIZ_AUX;

store 'CAUSAS' into 'CAUSAS.qvd' (qvd);  



